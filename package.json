{
  "name": "plaz4-plasmos",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.16.2",
    "base-64": "^0.1.0",
    "foundation-sites": "^6.4.3",
    "glamor": "^2.20.40",
    "moment": "^2.20.0",
    "react": "^15.6.2",
    "react-big-calendar": "^0.14.4",
    "react-color": "^2.13.8",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-sticky": "^6.0.1",
    "react-table": "^6.7.5",
    "react-toastify": "^2.2.2",
    "redux": "^3.7.2",
    "redux-localstorage": "^0.4.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "react-scripts": "0.9.5"
  },
  "scripts": {
    "build-css": "node-sass src/assets/scss -o src/assets/css --include-path src/assets/libs",
    "watch-css": "npm run build-css && node-sass src/assets/scss -o src/assets/css  --include-path src/assets/libs --watch --recursive",
    "start-js": "react-scripts start",
    "start": "HTTPS=true npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
